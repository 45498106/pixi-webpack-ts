{
  "name": "pixi-playground",
  "version": "3.0.0",
  "license": "CC-BY-NC-4.0",
  "author": "Lo√Øc TRUCHOT <loic.truchot.pro@gmail.com> (http://laonetics.io)",
  "description": "A pre installed environnement to work with pixi.js with webpack & typescript",
  "engines": {
    "node": "~8.0.0",
    "npm": "~5.0.0"
  },
  "main": "./index.html",
  "scripts": {
    "update": "rm -rf node_modules && npm cache verify && npm i",
    "gitrefresh": "git reset HEAD --hard && git clean -fd && git fetch --all --tags --prune",
    "start": "webpack-dev-server --hot --inline --port 5000 --contentBase './' --config ./webpack/dev.config.ts",
    "release": "webpack -p --config ./webpack/prod.config.ts",
    "deploy": "node ./dist/server.js"
  },
  "keywords": [
    "pixijs",
    "playground",
    "basic",
    "webpack",
    "typescript"
  ],
  "devDependencies": {
    "@types/body-parser": "^1.16.5",
    "@types/compression": "0.0.34",
    "@types/express": "^4.0.37",
    "@types/morgan": "^1.7.32",
    "@types/node": "^8.0.28",
    "@types/pixi.js": "^4.5.3",
    "css-loader": "^0.28.7",
    "file-loader": "^0.11.2",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.3.6",
    "tslint": "^5.7.0",
    "tslint-consistent-codestyle": "^1.6.0",
    "typescript": "^2.5.2",
    "typings": "^2.1.1",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "body-parser": "^1.18.1",
    "compression": "^1.7.0",
    "express": "^4.15.4",
    "morgan": "^1.8.2",
    "pixi.js": "^4.5.5"
  }
}
